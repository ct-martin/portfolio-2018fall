{
  "name": "Backplane",
  "version": "1.0.0",
  "description": "Node.js dynamic reverse proxy",
  "author": "CTMartin",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
